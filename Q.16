Q.16
Find LCM of two number 
public class Main
{
	public static void main(String[] args) {
		int n= 4;
		int m=6;
		int min=-1;
		if(n<m){
		    min=n;
		}
		else{
		    min=m;
		    
		}
		int ans=-1;
		for(int i=1;i<=min;i++){
		    if(n%i==0 && m%i==0){
		        ans=Math.max(ans,i);
		        
		    }
		}
		int lcm = (n*m)/ans;
		System.out.println(lcm);
	}
}

